# --------------------------Mutable----------------
def update(lst):
    print(id(lst))
    lst[1]=25
    print(id(lst))
    print('lst',lst)

lst=[10,20,30]
print(id(lst))
update(lst)  # passing the address not the value  (call by refference)
print("lst",lst)